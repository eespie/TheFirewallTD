; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="The Firewall TD"
run/main_scene="res://scenes/main/main.tscn"
boot_splash/image="res://splash-eboreal.png"
config/icon="res://icon.png"

[autoload]

file="*res://autoload/file.gd"
events="*res://autoload/events.gd"
scene="*res://autoload/scene.gd"
screen_mgr="*res://autoload/screen_mgr.gd"
game_mgr="*res://autoload/game_mgr.gd"

[display]

window/size/width=768
window/size/height=1365
window/size/resizable=false
window/size/test_width=768
window/size/test_height=1136
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[rendering]

environment/default_environment="res://default_env.tres"
